{
  "name": "@moonlightjs/prisma-module-generator",
  "version": "0.1.1-beta.2",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moonlightjs/prisma-module-generator.git"
  },
  "bugs": {
    "url": "https://github.com/Moonlightjs/prisma-module-generator/issues"
  },
  "homepage": "https://github.com/Moonlightjs/prisma-module-generator#readme",
  "author": {
    "email": "thangho250498@gmail.com",
    "name": "thangho98",
    "url": "https://thangho98.github.io/"
  },
  "scripts": {
    "release": "standard-version -a && npm i --package-lock-only && npm run module && cd module && npm publish --access public && git push --follow-tags",
    "prerelease-alpha": "standard-version --prerelease alpha && npm i --package-lock-only && npm run module && cd module && npm publish --access public --tag alpha && git push --follow-tags",
    "prerelease-beta": "standard-version -a --prerelease beta --releaseCommitMessageFormat && npm i --package-lock-only && npm run module && cd module && npm publish --access public --tag beta && git push --follow-tags",
    "start": "rimraf @generated && npm run build && npm run prisma generate",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc --build tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma": "prisma",
    "module": "npm run build && node ./module.js",
    "standard-version": "standard-version",
    "test": "ts-node ./test/test.ts"
  },
  "bin": {
    "prisma-module-generator": "./generator.js"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@moonlightjs/common": "0.1.2-beta.2",
    "@nestjs/config": "^2.3.1",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.0.0",
    "@prisma/generator-helper": "^4.0.0",
    "@prisma/internals": "^4.0.0",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "module": "^1.2.5",
    "pluralize": "^8.0.0",
    "ts-morph": "^15.1.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.4",
    "prisma": "^4.0.0",
    "rimraf": "^5.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
